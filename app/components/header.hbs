<nav class="border-gray-200 px-2 sm:px-4 py-2.5 rounded bg-gray-800"   {{did-insert this.didInsert}}>
  <div class="container flex flex-wrap justify-between items-center mx-auto">
  <a href="/" class="flex items-center">
      <img src="/img/logo.svg" class="mr-3 h-6 sm:h-9" alt="Dionysus Logo" />
      <span class="self-center text-xl font-semibold whitespace-nowrap text-white">Dionysus</span>
  </a>
  <div class="flex items-center md:order-2">
    {{#if this.msal.isAuthenticated}}
      <button type="button" class="flex mr-3 text-sm bg-gray-800 rounded-full md:mr-0 focus:ring-4 focus:ring-gray-600" id="user-menu-button" aria-expanded="false" type="button" data-dropdown-toggle="dropdown">
        <span class="sr-only">Open user menu</span>
        <img class="w-8 h-8 rounded-full" src="https://ui-avatars.com/api/?rounded=true&name={{this.msal.userFullName}}&background=random" alt="user photo">
      </button>
      <!-- Dropdown menu -->
      <div class="hidden z-50 my-4 text-base list-none rounded divide-y shadow bg-gray-700 divide-gray-600" id="dropdown">
        <div class="py-3 px-4">
          <span class="block text-sm text-white">{{this.msal.userFullName}}</span>
          <span class="block text-sm font-medium truncate text-gray-400">{{this.msal.userEmail}}</span>
        </div>
        <ul class="py-1" aria-labelledby="dropdown">
          <li>
            <LinkTo @route="tickets" class="block py-2 px-4 text-sm hover:bg-gray-600 text-gray-200 hover:text-white">My tickets</LinkTo>
          </li>
          <li>
            <LinkTo @route="signout" class="block py-2 px-4 text-sm hover:bg-gray-600 text-gray-200 hover:text-white">Sign out</LinkTo>
          </li>
        </ul>
      </div>
      {{else}}
      <button type="button" class="flex mr-3 text-sm bg-gray-800 rounded-full md:mr-0 focus:ring-4 focus:ring-gray-600" id="user-menu-button" aria-expanded="false" type="button" data-dropdown-toggle="dropdown">
        <LinkTo @route="signin" class="block py-2 px-4 text-sm hover:bg-gray-600 text-gray-200 hover:text-white">Sign in</LinkTo>
      </button>
    {{/if}}
      <button data-collapse-toggle="mobile-menu-2" type="button" class="inline-flex items-center p-2 ml-1 text-sm text-gray-500 rounded-lg md:hidden hover:bg-gray-100 focus:outline-none focus:ring-2 focus:ring-gray-200 dark:text-gray-400 dark:hover:bg-gray-700 dark:focus:ring-gray-600" aria-controls="mobile-menu-2" aria-expanded="false">
      <span class="sr-only">Open main menu</span>
      <svg class="w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path></svg>
      <svg class="hidden w-6 h-6" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
    </button>
  </div>
  <div class="hidden justify-between items-center w-full md:flex md:w-auto md:order-1" id="mobile-menu-2">
    <ul class="flex flex-col mt-4 md:flex-row md:space-x-8 md:mt-0 md:text-sm md:font-medium">
      <li>
        <a href="#" class="block py-2 pr-4 pl-3  bg-blue-700 rounded md:bg-transparent md:text-blue-700 md:p-0 text-white" aria-current="page">Home</a>
      </li>
    </ul>
  </div>
  </div>
</nav>